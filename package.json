  "bugs": {
    "url": "https://github.com/BenedyktT/gastrofreak/issues"
  },
  "homepage": "https://github.com/BenedyktT/gastrofreak#readme",
  "devDependencies": {
    "@babel/node": "^7.8.7",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^5.1.0",
    "esm": "^3.2.25",
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
